package DSA_assign1;

public class primefactors {
    public static void main(String[] args) {
        int n=315;
        Printprimefactors(n);
    }
    public static void Printprimefactors(int n){
        for(int i=2; i<=n;i++){
            while (n%i==0){
                n=n/i;
                System.out.println(i);
            }
        }
    }
}
